<krpano version="1.17.4">

	<!-- the skin -->
	<include url="skin/defaultskin.xml" />
    <include url="plugins/showtext.xml" />

	<!-- view settings -->
	<view hlookat="0" vlookat="0" maxpixelzoom="1.0" fovmax="150" limitview="auto" />


	<preview url="2th_Floor_-_Corridor_End.tiles/preview.jpg" />

	<image>
		<cube url="2th_Floor_-_Corridor_End.tiles/pano_%s.jpg" />
		<mobile>
			<cube url="2th_Floor_-_Corridor_End.tiles/mobile_%s.jpg" />
		</mobile>
	</image>

	<hotspot name="snd_Floor_Corridor_End"
             ath="116.15095648491788"
             atv="-4.565774306289762"
             url="plugins/sensor-128.png"
             alpha="0.4"
             scale="0.7"
             zoom="true"
             onhover="set(alpha,1.0);showtext(get(room));"
             onclick="set(alpha,1.0);showtext(get(room), mobile);"
             onout="set(alpha,0.4);"
            />

</krpano>
